{
  "name": "storage-manager-js",
  "version": "4.0.4",
  "description": "A little way to control Cookies, LocalStorage and SessionStorage without tears",
  "main": "./build/index.js",
  "scripts": {
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "build": "bili",
    "rm": "rm -rf build",
    "prepublish": "npm-run-all -s compile test",
    "compile": "npm-run-all -s rm build",
    "test": "jest"
  },
  "keywords": [
    "cookie",
    "cookies",
    "browser",
    "localstorage",
    "sessionstorage",
    "client",
    "js-cookie",
    "storage",
    "manager",
    "localStorage",
    "sessionStorage"
  ],
  "homepage": "https://github.com/g4rcez/storage-manager-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/g4rcez/storage-manager-js"
  },
  "author": "Allan Garcez <allan.f.garcez@gmail.com> (https://github.com/g4rcez)",
  "license": "MIT",
  "dependencies": {
    "jest": "^26.0.1"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "bili": "^5.0.2",
    "browser-resolve": "^1.11.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rollup-plugin-typescript2": "^0.27.1",
    "tslib": "^2.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  }
}
