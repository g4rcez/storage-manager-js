{
  "name": "storage-manager-js",
  "version": "3.1.5",
  "description": "A little way to control Cookies, LocalStorage and SessionStorage without tears",
  "main": "./build/index.js",
  "scripts": {
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "build": "bili",
    "rm": "rm -rf build",
    "prepublish": "npm-run-all -s compile test",
    "compile": "npm-run-all -s rm build",
    "test": "npx jest"
  },
  "keywords": [
    "cookie",
    "cookies",
    "browser",
    "localstorage",
    "sessionstorage",
    "client",
    "js-cookie",
    "storage",
    "manager",
    "localStorage",
    "sessionStorage"
  ],
  "homepage": "https://github.com/g4rcez/storage-manager",
  "repository": {
    "type": "git",
    "url": "https://github.com/g4rcez/storage-manager"
  },
  "author": "Allan Garcez <allan.f.garcez@gmail.com> (https://github.com/g4rcez)",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "bili": "^4.8.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rollup-plugin-typescript2": "^0.26.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  }
}