{
  "name": "storage-manager-js",
  "version": "4.0.7",
  "description": "A little way to control Cookies, LocalStorage and SessionStorage without tears",
  "main": "./build/index.js",
  "scripts": {
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "build": "bili",
    "rm": "rm -rf build",
    "prepublish": "yarn compile && yarn test",
    "compile": "yarn rm && yarn build",
    "test": "jest"
  },
  "keywords": [
    "cookie",
    "cookies",
    "browser",
    "localstorage",
    "sessionstorage",
    "client",
    "js-cookie",
    "storage",
    "manager",
    "localStorage",
    "sessionStorage"
  ],
  "homepage": "https://github.com/g4rcez/storage-manager-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/g4rcez/storage-manager-js"
  },
  "author": "Allan Garcez <allan.f.garcez@gmail.com> (https://github.com/g4rcez)",
  "license": "MIT",
  "dependencies": {
    "jest": "26.6.3"
  },
  "devDependencies": {
    "@types/mocha": "8.2.2",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "bili": "5.0.5",
    "browser-resolve": "2.0.0",
    "prettier": "2.2.1",
    "rollup-plugin-typescript2": "0.30.0",
    "tslib": "2.2.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.2.4"
  }
}
